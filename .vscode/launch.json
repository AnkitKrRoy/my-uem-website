{
    "version": "0.2.0",
    "configurations": [
    
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Express App",
        "skipFiles": ["<node_internals>/**"],
        "program": "${MY-UEM-WEBSITE-MASTER}/app.js", // Replace with the name of your main application file
        "cwd": "${MY-UEM-WEBSITE-MASTER}",
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal"
      }
    ]
  }